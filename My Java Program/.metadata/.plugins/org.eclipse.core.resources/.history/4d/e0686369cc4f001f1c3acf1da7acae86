package Java_Basic_Concepts;

public class basic_types {
	
	public static void main(String[] args) {
		int myInt=7;
		double shoesSize=9.5;
		char myinitial='J';
		
		String myName="John";
		
		double result=myInt*shoesSize;
		
		System.out.println(result);
		
		burp();
		
		if(myInt==7) {
			System.out.println("Hello");	
		}
		for(int i=0;i<10;i++) {
			System.out.print("a");
		}
		cat.dingdong();
		cat myCat=new cat();
		myCat.name="Fred";
		myCat.age=6;
		
		
		
	}
	private static void burp() {
		System.out.println("Burrrrp");
	}
	private static void printName(String name) {
		System.out.println("My name is"+name);
	}
}
